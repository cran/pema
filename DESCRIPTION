Package: pema
Title: Penalized Meta-Analysis
Version: 0.1.0
Authors@R: c(
    person(c("Caspar", "J"), 
             "van Lissa",
             role = c("aut", "cre"),
             email = "c.j.vanlissa@uu.nl",
             comment = c(ORCID = "0000-0002-0808-5024")),
    person(c("Sara", "J"), "van Erp", role = c("aut"))
